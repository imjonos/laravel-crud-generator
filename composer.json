{
    "name": "codersstudio/crud",
    "description": ":description",
    "license": "Commercial",
    "authors": [
        {
            "name": "Coders Studio",
            "email": "info@coders.studio",
            "homepage": "https://coders.studio"
        }
    ],
    "homepage": "https://gitlab.com/codersstudio/crud",
    "keywords": ["Laravel", "CRUD"],
    "require": {
        "illuminate/support": "~5",
        "doctrine/dbal": "*",
        "spatie/laravel-medialibrary": "*",
        "symfony/process": "*",
        "maatwebsite/excel": "*"

    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CodersStudio\\CRUD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodersStudio\\CRUD\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodersStudio\\CRUD\\CRUDServiceProvider"
            ],
            "aliases": {
                "CRUD": "CodersStudio\\CRUD\\Facades\\CRUD"
            }
        }
    }
}
